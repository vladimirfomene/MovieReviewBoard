type Movie {
    id: ID!
    title: String!
    director: Director!
    rating: Long
    releaseDate: String!
}

type Query{
    findAllMovies : [Movie]!
    countMovies : Long!
}

type Mutation{
    updateMovieRating(movieId: Long, vote: Long!) : Long!
    newMovie(title: String!, directorId: ID!, releaseDate: String!, rating: Long) : Movie!
}